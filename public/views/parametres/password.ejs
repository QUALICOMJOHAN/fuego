<%- include('../partials/topbar/topbar.ejs') %>

<div class="row">
    <div class="col-md-6">
        <h2>Génération mot de passe</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
    <% if(locals.errorObject && errorObject.error) {%>
        <p><%= errorObject.error_message %></p>
    <% } else { %>
        <p>mdp : <%= password.text %></p>
        <p>hash : <%= password.hash %></p>
    <% } %>
    </div>
</div>
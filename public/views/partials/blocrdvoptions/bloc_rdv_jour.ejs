<div class="ctn_rdv_auj <%= rdv.Etat != null ? 'valide' : rdv.statut == 1 ? 'confirme' : '' %>" id="<%= rdv.id %>">
                <div class="bloc_rdv_infosclient">
                 <h6><%= rdv.date %></h6>
                 <p><%= rdv.Client.nom %> <%= rdv.Client.prenom %></p>
                 <p><%= rdv.Client.cp %> <%= rdv.Client.ville %></p>
                 <p class="adresse"><%= rdv.Client.adresse %></p>
                 <p><%= rdv.Etat != null ? rdv.Etat.nom : '' %></p>
                 <p><%= rdv.User != null ? rdv.User.nom : ''  %> <%= rdv.User != null ? rdv.User.prenom : ''  %></p>
                 </div>
                 <div class="options_template">
                     <% if(typeof options_template != 'undefined'){ %>
                        <%- include(options_template) %>
                     <% } %>
                 </div>
</div>
<% if(typeof this.isAffichageTuile !== 'undefined' && !isAffichageTuile) { %>
    <div id="<%= rdv.id %>" class="btn_item2 un hover_btn3">
        <i class="fas fa-user-tag"></i>
    </div>
    <div class="btn_item2 deux hover_btn3">
        <a target="_blank" href="https://www.google.fr/maps/search/<%= rdv.Client.adresse.split(' ').join('+')+'+'+rdv.Client.cp %>"><i class="fas fa-map-signs"></i></a>
    </div>
    <div id="<%= rdv.id %>" class="btn_item2 trois hover_btn3">
        <i class="fas fa-file-pdf"></i>
    </div>
<% } else { %>
    <div class="options_rdv_ctn">
        <div class="open">
            <i class="fas fa-plus"></i>
        </div>

        <div class="btn_caches ">
            <div id="<%= rdv.id %>" class="btn_item un rdv_btn_anim1_retour hover_btn3">
                <i class="fas fa-user-tag"></i>
            </div>
            <div class="btn_item deux rdv_btn_anim2_retour hover_btn3">
                <a target="_blank" href="https://www.google.fr/maps/search/<%= rdv.Client.adresse.split(' ').join('+')+'+'+rdv.Client.cp %>"><i class="fas fa-map-signs"></i></a>
            </div>
            <div id="<%= rdv.id %>" class="btn_item trois rdv_btn_anim3_retour hover_btn3">
                <i class="fas fa-file-pdf"></i>
            </div>
        </div>
    </div>
<% } %>
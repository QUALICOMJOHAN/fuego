    <table class="historique_tableau">
        <thead>
            <th  class="tableau_statut">Statut</th>
            <th  class="tableau_sousstatut">Sous Statut</th>
            <th  class="tableau_datetraitement">Date de Traitement</th>
            <th class="tableau_dateevenement">Date de l'événement</th>
            <th class="tableau_commentaire">Commentaire</th>
            <th class="tableau_telec">Téléconseiller</th>
        </thead>
        <tbody>
            <% findedClient.Historiques.forEach((histo) => { %>
                <tr id="historique_ligne">
                    <td><%= histo.Action.nom %></td>
                    <td><%= histo.sousstatut %></td>
                    <td><%= histo.createdAt %></td>
                    <td><%= histo.dateevent %></td>
                    <td class="commentaire3"><%= histo.commentaire %></td>
                    <td><%= histo.User.nom+' '+histo.User.prenom %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
<div class="row">
    <div class="col-md-6">
        <h2 class="day">Jour 00 MoisMois</h2>
        
        
<div class="ctn_table">

<table>
        <thead>
            <th class="tableau_commercial">Commercial</th>
            <th class="tableau_matin">Matin</th>
            <th class="tableau_midi">Midi</th>
            <th class="tableau_soir">Soir</th>
            <th class="tableau_total">Total</th>
        </thead>
        <tbody>
            <% findedUsers.forEach((user) => { %>
            <tr id="com_<%= user.id %>">
                <td><%= user.nom+' '+user.prenom %></td>
                <td class="m"><%= %></td>
                <td class="am"><%= %></td>
                <td class="s"><%= %></td>
                <td class="t"><%= user.objectif %></td>
            </tr>
            <% }) %>
            <tr>
                <td class="rdvPris" colspan="4"></td>
                <td class="totalr"></td>
            </tr>
        </tbody>
    </table>
    
